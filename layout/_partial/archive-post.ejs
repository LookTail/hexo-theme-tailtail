<section class="archive-post">
    <h1>
        <%= year %>
    </h1>
    <ul>
      <% posts.sort((l, r) => r.date - l.date) %>
      <% posts.forEach(post => { %>
        <li>
          <time><%= post.date.format("MMM Do") %></time>
          <a href="<%= url_for(post.path) %>">
            <h2><%= post.title %></h2>
          </a>
        </li>
      <% }) %>
    </ul>
</section>